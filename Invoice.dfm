  object ibdsMainInvoicesList: TIBDataSet
    Database = frmDM.dbMain
    Transaction = trMain
    AfterDelete = ibdsMainInvoicesListAfterDelete
    AfterScroll = ibdsMainInvoicesListAfterScroll
    OnCalcFields = ibdsMainInvoicesListCalcFields
    OnUpdateRecord = ibdsMainInvoicesListUpdateRecord
    CachedUpdates = True
    DeleteSQL.Strings = (
      'delete from INVOICE_HEADER'
      'where'
      '  ID = :OLD_ID')
    SelectSQL.Strings = (
      
        'select IH.ID, P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_NAME MASTER_' +
        'NAME,'
      
        'IH.NUMBER_PREFIX||cast(IH.NUMBER as varchar(30))||IH.NUMBER_SUFF' +
        'IX NUMBER_STR,'
      
        'IH.INVOICE_DATE, E.NAME ENTERPRISE_NAME, CNTR.NAME CONTRACTOR_NA' +
        'ME,'
      'IH.CLOSED, IH.AMOUNT, C.SYMBOL,'
      
        'C.DECIMALS_IN DECIMALS, IH.SALDO_MANY, IH.SALDO_GOODS, IH.BLACK,' +
        ' IH.DOUBLE_SOSTAV, IH.OPPOSITE_SOSTAV_ID,'
      
        'InvS.NAME SETUP_NAME, IH.CONTRACTOR_ID, UR.DELETE_RIGHT, UR.CLOS' +
        'E_RIGHT, UR.CREATE_RIGHT'
      'from PERSONS P, ENTERPRISES E, CONTRACTORS CNTR,'
      
        'INVOICE_SETUP InvS, CURRENCY C, INVOICE_HEADER IH, USERS U, USER' +
        '_RIGHTS_FOR_INVOICE UR'
      'where (IH.MASTER_ID=P.ID) and (IH.ENTERPRISE_ID=E.ID) and'
      '(IH.CONTRACTOR_ID=CNTR.ID) and (IH.INVOICE_SETUP_ID=InvS.ID) and'
      
        '(InvS.IN_OUT=:In_Out) and (C.ID=IH.CURRENCY_ID) and (U.PERSON_ID' +
