    SelectSQL.Strings = (
      'select InvS.ID, InvS.NAME, InvS.IN_OUT'
      'from INVOICE_SETUP InvS'
      'where (InvS.IN_OUT in (1,2)) and'
      
        'exists(select * from GET_INVOICE_SETUP(InvS.ENTERPRISE_ID,InvS.I' +
        'N_OUT,:PERSON_ID) GIS'
