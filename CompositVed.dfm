  object ibdsInvoiceTypes: TIBDataSet
    Database = frmDM.dbMain
    Transaction = trMain
    BeforeClose = ibdsInvoiceTypesBeforeClose
    BeforeOpen = ibdsInvoiceTypesBeforeOpen
