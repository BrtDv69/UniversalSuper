  object ibdsBalansGrouped: TIBDataSet
    Database = frmDM.dbMain
    Transaction = trMain
    AfterOpen = ibdsBalansGroupedAfterOpen
    BeforeClose = ibdsBalansGroupedBeforeClose
    OnCalcFields = ibdsBalansGroupedCalcFields
    SelectSQL.Strings = (
      
        'select C.NAME, CUR.ID, CUR.SYMBOL, CUR.DECIMALS_IN DECIMALS, SUM' +
        '(TOIB.AMOUNT) AMOUNT, -SUM(TOIB.SALDO_GOODS) SALDO_GOODS, SUM(TO' +
        'IB.SALDO_MANY) SALDO_MANY, IH.CONTRACTOR_ID'
      
        'from CURRENCY CUR, CONTRACTORS C, INVOICE_HEADER IH, TMP_OPEN_IN' +
        'VOICES_BALANS TOIB'
      
        'where (TOIB.CREATOR=current_user) and (TOIB.INVOICE_ID=IH.ID) an' +
        'd (C.ID=IH.CONTRACTOR_ID) and (CUR.ID=IH.CURRENCY_ID)'
      
        'group by C.NAME, CUR.ID, CUR.SYMBOL, CUR.DECIMALS_IN, IH.CONTRAC' +
        'TOR_ID'
      'order by 1')
    Left = 25
    Top = 63
    object ibdsBalansGroupedNAME: TIBStringField
      FieldName = 'NAME'
      Required = True
      Size = 100
    end
    object ibdsBalansGroupedAMOUNT: TFloatField
      FieldName = 'AMOUNT'
    end
