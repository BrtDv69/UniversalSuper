    InsertSQL.Strings = (
      'insert into GOODS'
      '  (STORE_ID, ARTICUL_ID, MADE_IN, MIN_AMOUNT,'
      '   SERTIFICATE_INFORMATION, VALID_DATE, COUNTRY_ID,'
      '   DECLARATION_NUMBER, COMMENTS)'
      'values'
      '  (:STORE_ID, :ARTICUL_ID, :MADE_IN, :MIN_AMOUNT,'
      '   :SERTIFICATE_INFORMATION, :VALID_DATE, :COUNTRY_ID,'
      '   :DECLARATION_NUMBER, :COMMENTS)')
    SelectSQL.Strings = (
      'select G.ID, G.ARTICUL_ID, A.NAME ARTICUL_NAME, G.MADE_IN,'
      'G.STORE_ID, S.NAME STORE_NAME, S.ENTERPRISE_ID,'
      'E.NAME ENTERPRISE_NAME, G.PRICE, G.PACK_VALUE, G.IN_SETS,'
      
        'G.TOTAL_REAL, G.TOTAL_WHITE TOTAL, G.MIN_AMOUNT, G.LAST_BUY_PRIC' +
        'E,'
      'G.SERTIFICATE_INFORMATION, G.VALID_DATE, G.COUNTRY_ID,'
      'C.NAME COUNTRY_NAME, G.DECLARATION_NUMBER, G.COMMENTS'
      
        'from ENTERPRISES E join STORES S on (E.ID=S.ENTERPRISE_ID) join ' +
        'GOODS G on'
      
        '(S.ID=G.STORE_ID) and (G.ARTICUL_ID=:ARTICUL_ID) and (G.MADE_IN=' +
        ':MADE_IN)'
      
        'join ARTICULS A on (A.ID=G.ARTICUL_ID) left outer join COUNTRIES' +
        ' C on'
      '(C.ID=G.COUNTRY_ID)'
      'order by 6 asc, 8 asc')
    ModifySQL.Strings = (
      'update GOODS'
      'set'
      '  PRICE = :PRICE,'
      '  PACK_VALUE = :PACK_VALUE,'
