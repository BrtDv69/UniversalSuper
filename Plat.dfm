    BeforeOpen = ibdsMainPrimaryPaymentsListBeforeOpen
    OnCalcFields = ibdsMainPrimaryPaymentsListCalcFields
    OnUpdateRecord = ibdsMainPrimaryPaymentsListUpdateRecord
    CachedUpdates = True
    DeleteSQL.Strings = (
      'delete from PRIMARY_PAYMENTS'
      'where'
      '  ID = :OLD_ID')
    SelectSQL.Strings = (
      
        'select PP.ID, P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_NAME MASTER_' +
        'NAME, PS.REASON_TYPE, PS.MOVEMENT_TYPE_NEW, PS.PAYMENT_TYPE,'
      
        'PP.NUMBER_PREFIX||cast(PP.NUMBER as varchar(30))||PP.NUMBER_SUFF' +
        'IX NUMBER_STR,'
      
        'PP.PAYMENT_DATE, ECD.NAME||'#39' '#39'||S.NAME||'#39' '#39'||E.NAME ENTERPRISE_C' +
        'ASH_DESK_NAME,'
      'CNTR.NAME CONTRACTOR_NAME, PP.AMOUNT, C.SYMBOL,'
      'C.DECIMALS_IN DECIMALS, PP.BLACK,'
      'PS.NAME SETUP_NAME, UR.DELETE_RIGHT'
      'from PRIMARY_PAYMENTS PP, PERSONS P, ENTERPRISES E,'
      'CONTRACTORS CNTR, PRIMARY_SETUP PS, STORES S,'
      
        'ENTERPRISE_CASH_DESKS ECD, CURRENCY C, USERS U, USER_RIGHTS_FOR_' +
        'PRIMARY UR'
      
        'where (PS.MOVEMENT_TYPE=:MOVEMENT_TYPE) and (PS.IN_OUT=:IN_OUT) ' +
        'and'
      
        '(U.PERSON_ID=:PERSON_ID) and ((UR.OWN_ONLY=0) or ((UR.OWN_ONLY=1' +
        ') and'
      
        '(PP.MASTER_ID=:PERSON_ID))) and (U.SECURITY_LEVEL!<PP.SECURITY_L' +
        'EVEL) and'
      '(PP.MASTER_ID=P.ID) and (PP.ENTERPRISE_ID=E.ID) and'
      
        '(PP.ENTERPRISE_STORE_ID=S.ID) and (PP.CURRENCY_ID=C.ID) and (PP.' +
        'ENTERPRISE_CASH_DESK_ID=ECD.ID) and'
      '(PP.CONTRACTOR_ID=CNTR.ID) and (PP.PRIMARY_SETUP_ID=PS.ID) and'
      
        '(UR.USER_ID=U.ID) and (UR.PRIMARY_SETUP_ID=PP.PRIMARY_SETUP_ID) ' +
        'and (UR.BLACK=PP.BLACK)'
      'order by 7, 6, 2, 8, 9')
