    SelectSQL.Strings = (
      
        'select IH.ID, P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_NAME MASTER_' +
        'NAME,'
      
        'IH.NUMBER_PREFIX||cast(IH.NUMBER as varchar(30))||IH.NUMBER_SUFF' +
        'IX NUMBER_STR,'
      
        'IH.INVOICE_DATE, EF.NAME ENTERPRISE_NAME, SF.NAME ENTERPRISE_STO' +
        'RE_NAME,'
      'ET.NAME CONTRACTOR_NAME, ST.NAME CONTRACTOR_STORE_NAME,'
      'IH.CLOSED, IH.AMOUNT,'
      
        'C.SYMBOL, C.DECIMALS_IN DECIMALS, InvS.NAME SETUP_NAME, UR.DELET' +
        'E_RIGHT'
      
        'from PERSONS P, ENTERPRISES EF, ENTERPRISES ET, STORES SF, STORE' +
        'S ST,'
      
        'INVOICE_SETUP InvS, CURRENCY C, INVOICE_HEADER IH, USERS U, USER' +
        '_RIGHTS_FOR_INVOICE UR'
      'where (IH.MASTER_ID=P.ID) and (IH.ENTERPRISE_ID=EF.ID) and'
      '(IH.CONTRACTOR_ID=-ST.ID) and (IH.ENTERPRISE_STORE_ID=SF.ID) and'
      '(ET.ID=ST.ENTERPRISE_ID) and (IH.INVOICE_SETUP_ID=InvS.ID) and'
      
        '(InvS.IN_OUT=3) and (C.ID=IH.CURRENCY_ID) and (U.PERSON_ID=:PERS' +
        'ON_ID) and'
      
        '(UR.USER_ID=U.ID) and (UR.INVOICE_SETUP_ID=IH.INVOICE_SETUP_ID) ' +
        'and'
      '(UR.BLACK=IH.BLACK) and'
      
        '((UR.OWN_ONLY=0) or ((UR.OWN_ONLY=1) and (IH.MASTER_ID=:PERSON_I' +
        'D))) and'
      '(U.SECURITY_LEVEL!<IH.SECURITY_LEVEL)'
