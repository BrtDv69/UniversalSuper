        '  (ID, INVOICE_SETUP_ID, CONTRACT_ID, MASTER_ID, SECURITY_LEVEL,' +
        ' NUMBER_PREFIX, '
      
        '   NUMBER, NUMBER_SUFFIX, INVOICE_DATE, ENTERPRISE_ID, ENTERPRIS' +
        'E_STORE_ID, '
      '   BLACK,'
      '   CONTRACTOR_ID, CURRENCY_ID, CURRENCY_RATE, CLOSED, COMMENTS)'
      'values'
      
        '  (:ID, :INVOICE_SETUP_ID, :CONTRACT_ID, :MASTER_ID, :SECURITY_L' +
        'EVEL, :NUMBER_PREFIX, '
      
        '   :NUMBER, :NUMBER_SUFFIX, :INVOICE_DATE, :ENTERPRISE_ID, :ENTE' +
        'RPRISE_STORE_ID, '
      '   1,'
      
        '   :CONTRACTOR_ID, :CURRENCY_ID, :CURRENCY_RATE, :CLOSED, :COMME' +
        'NTS)')
    RefreshSQL.Strings = (
      'select IH.ID, IH.INVOICE_SETUP_ID, InvS.NAME SETUP_NAME,'
      'InvS.NUMBER_FORMAT, InvS.NUMBER_RESTART_PERIOD,'
      'InvS.PRICE_FORMULA, IH.CONTRACT_ID,'
      
        'IH.MASTER_ID, P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_NAME MASTER_' +
        'NAME,'
      'IH.SECURITY_LEVEL, IH.NUMBER_PREFIX, IH.NUMBER,'
      'IH.NUMBER_SUFFIX, IH.INVOICE_DATE, IH.ENTERPRISE_ID,'
      
        'E.NAME ENTERPRISE_NAME, IH.ENTERPRISE_STORE_ID, S.NAME ENTERPRIS' +
        'E_STORE_NAME,'
      'IH.CONTRACTOR_ID, C.NAME CONTRACTOR_NAME,'
      'IH.CURRENCY_ID, CUR.NAME CURRENCY_NAME, CUR.DECIMALS_IN,'
      'CUR.DECIMALS_OUT, IH.CURRENCY_RATE, IH.CLOSED, IH.COMMENTS'
      'from INVOICE_HEADER IH, INVOICE_SETUP InvS, PERSONS P,'
      'ENTERPRISES E, STORES S, CURRENCY CUR, CONTRACTORS C'
      'where (IH.ID=:ID) and (IH.INVOICE_SETUP_ID=InvS.ID) and'
      '(IH.MASTER_ID=P.ID) and (IH.ENTERPRISE_ID=E.ID) and'
